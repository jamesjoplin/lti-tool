{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core",
    "packages/hono",
    "packages/memory",
    "packages/dynamodb",
    "packages/mysql",
    "packages/postgresql"
  ],
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "hcl",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "tsx",
    "typescript"
  ],
  "entryPointStrategy": "packages",
  "packageOptions": {
    "entryPoints": ["src/index.ts"],
    "excludePrivate": true,
    "excludeInternal": true,
    "validation": {
      "notExported": false
    }
  },
  "out": "docs",
  "name": "lti-tool Documentation",
  "includeVersion": true,
  "githubPages": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "visibilityFilters": {
    "protected": true,
    "@internal": false
  }
}
